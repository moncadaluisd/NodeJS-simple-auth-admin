<script type="text/javascript">

    $.ajax({
      url: '/api/notify',
      type: 'POST',
      data: {id: '<%= user.id %>'}
    })
    .done(function(data) {
      console.log(data);
      for (var i = 0; i < data.length; i++) {

        $('#data-table').append('<tr><td data-title="market">'+  data[i].market+'</td><td data-title="status">'+  data[i].status+'</td><td data-title="buy date">'+  data[i].date+'</td><td data-title="buy price">'+  data[i].price+'</td><td data-title="sell Date">'+  data[i].sDate+'</td><td data-title="Current/sell Price">'+  data[i].sellP+'</td><td data-title="Profit">'+  data[i].profit+'%</td> </tr>');
      }
      for (var i = 0; i < 5; i++) {

        $('#notify').append(`<li>
            <div class="alert alert-info clearfix">
                <span class="alert-icon"><i class="fa fa-bolt"></i></span>
                <div class="noti-info">
                    <a href="#">New Notification ` + data[i].market +`</a>
                </div>
            </div>
        </li>`);
      }

      $('#cantidad').html(data.length);
    })


    .fail(function() {
      console.log("error");
    })
    .always(function() {
      console.log("complete");
    });

  cargar_push();
</script>
